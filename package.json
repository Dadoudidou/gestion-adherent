  "main": "build/server.js",
  "betterScripts": {
    "build:client": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config webpack.client.config.js --progress"
    },
    "build:server": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config webpack.server.config.js --progress"
    },
    "start:appli": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "nodemon build/server.js --watch build"
    }
  },
  "scripts": {
    "build": "better-npm-run build:server && better-npm-run build:client",
    "start": "node build/server.js",
    "console": "webpack --config webpack.console.config.js --progress && node build/console.js",
    "devold": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:appli\" ",
    "dev:client": "webpack --config webpack.client.config.js --watch --progress",
    "dev:server": "webpack --config webpack.server.config.js --watch --progress",
    "dev:appli": "nodemon build/server.js --watch build/server.js",
    "storybook": "start-storybook -p 6006",
    "dev": "webpack --config config/webpack.config.js --watch --progress"
  },
