{
  "name": "gestionadherent",
  "version": "1.0.0",
  "description": "Gestion adh√©rent",
  "main": "index.js",
  "betterScripts": {
    "build:client": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config webpack.client.config.js --progress"
    },
    "build:server": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config webpack.server.config.js --progress"
    },
    "start:appli": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "nodemon build/server.js --watch build"
    }
  },
  "scripts": {
    "build": "better-npm-run build:server && better-npm-run build:client",
    "start": "node build/server.js",
    "console": "webpack --config webpack.console.config.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:appli\" ",
    "dev:client": "webpack --config webpack.client.config.js --watch",
    "dev:server": "webpack --config webpack.server.config.js --watch",
    "dev:appli": "nodemon build/server.js --watch build/server.js"
  },
