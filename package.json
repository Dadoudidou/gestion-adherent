{
  "name": "gestionadherent",
  "version": "1.0.0",
  "description": "Gestion adh√©rent",
  "main": "index.js",
  "betterScripts": {
    "start:dev": {
      "command": "webpack-dev-server --config ./webpack.client.config.js --devtool cheap-eval-source-map --inline --hot --port 9080 --content-base build/"
    },
    "build:client": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config ./webpack.client.config.js -p"
    },
    "build:server": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "webpack --config ./webpack.server.config.js -p"
    }
  },
  "scripts": {
    "dev-server": "better-npm-run start:dev",
    "build-client": "better-npm-run build:client",
    "build-server": "better-npm-run build:server",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\" \"npm run start:appli\" ",
    "start:client": "webpack --config webpack.client.config.js --watch --progress",
    "start:server": "webpack --config webpack.server.config.js --watch",
    "start:appli": "nodemon build/server.js --watch build",
    "stopall": "pm2 delete all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon './../../../build/server.js' --watch './../../../build'"
  },
  "author": "David Violet",
  "license": "MIT",
